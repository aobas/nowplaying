<?xml version="1.0"?>
<SettingClass xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <TweetText>NowPlaying $TITLE - $ARTIST(Album:$ALBUM PlayedCount:$COUNTtimes) #nowplaying #なうぷれ</TweetText>
  <EnableAutoTweet>true</EnableAutoTweet>
  <EnableTweetWait>true</EnableTweetWait>
  <EnableSameAlbumNoTweet>false</EnableSameAlbumNoTweet>
  <TweetWaitSeconds>10</TweetWaitSeconds>
  <CheckForUpdate>true</CheckForUpdate>
  <ShowUpdate>true</ShowUpdate>
  <SendOnEnterKey>false</SendOnEnterKey>
  <TweetInterval>2</TweetInterval>
  <TweetIntervelEnabled>true</TweetIntervelEnabled>
</SettingClass>